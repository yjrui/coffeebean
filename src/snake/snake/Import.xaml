<Window x:Class="snake.Import"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="导入数据" Height="300" Width="300">
    <StackPanel>
        <GroupBox Header="设备">
            <StackPanel>
                <ListBox Name="lbDevice" Height="80"/>
                <StackPanel Orientation="Horizontal">
                    <Button Name="btnRefresh" Content="刷新" Width="50" HorizontalAlignment="Left" Margin="5" Click="btnRefresh_Click" />
                    <Button Name="btnImport" Content="导入" Width="50" HorizontalAlignment="Left" Margin="5" Click="btnImport_Click" />
                </StackPanel>
            </StackPanel>
        </GroupBox>
        <GroupBox Header="结果">
            <TextBox Name="lbResult" Height="100" VerticalScrollBarVisibility="Auto" TextWrapping="Wrap"/>
        </GroupBox>
    </StackPanel>
</Window>
